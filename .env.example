# Example environment file for docker-compose in recreacion_linux/
# Copy to .env and adjust values on the HOST.

HEADLESS=true
DEBUG_SCRAPER=false
BLOCK_RESOURCES=true
SLOW_MO=100
TIMEOUT_MS=120000

# Google credentials JSON inside the container
GOOGLE_APPLICATION_CREDENTIALS=/app/recreacion_linux/credentials.json
# Interrapidisimo tracking map path inside the container
INTER_MAP_PATH=/app/recreacion_linux/interrapidisimo_traking_map.json

# Optional business settings
DRIVE_FOLDER_ID=
SPREADSHEET_NAME=seguimiento
TZ=America/Bogota
DAILY_REPORT_PREFIX=Informe_

# Optional proxy settings
# PROXY_SERVER=socks5://host:port
# PROXY_USERNAME=user
# PROXY_PASSWORD=pass
